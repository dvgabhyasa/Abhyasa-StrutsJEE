<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>edu.abhyasa.strutsops</groupId>
	<artifactId>Abhyasa-StrutsJEE-Scaffold-Macz</artifactId>
	<version>1.0.0-SNAPSHOT</version>

	<!-- Output Build Type -->
	<packaging>war</packaging>

	<!-- Name Desc Url -->
	<name>Struts Macz</name>
	<description>This is a Cameron Macenzee playlist implementation from
		scaffold to various usecases.</description>
	<url>http://struts.abhyasa.edu</url>

	<dependencies>
		<!-- Struts-Core dependency from :
		https://central.sonatype.com/artifact/org.apache.struts/struts2-core/6.3.0/overview -->
		<dependency>
			<groupId>org.apache.struts</groupId>
			<artifactId>struts2-core</artifactId>
			<version>2.5.32</version>
			<!--<version>6.3.0</version>-->
		</dependency>

		<!-- Struts2-Convention from
		https://central.sonatype.com/artifact/org.apache.struts/struts2-convention-plugin/2.5.32 -->
		<dependency>
			<groupId>org.apache.struts</groupId>
			<artifactId>struts2-convention-plugin</artifactId>
			<version>2.5.32</version>
		</dependency>

		<!--Servlet API from : https://central.sonatype.com/artifact/javax.servlet/javax.servlet-api/3.1.0 -->

		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<version>3.1.0</version>
		</dependency>

		<build>
			<plugins>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugins>

		</build>


	</dependencies>
</project>